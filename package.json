{
  "name": "gatsby-theme-undataforum",
  "version": "0.0.0",
  "description": "Gatbsy theme for UN World Data Forum websites.",
  "author": "Thilo Maier <maier@un.org>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "gatsby develop",
    "build": "rm -rf ./public && gatsby build",
    "format": "pretty-quick",
    "inspect": "node --inspect-brk node_modules/.bin/gatsby develop",
    "lint": "eslint --ext .jsx,.js --ignore-path .gitignore .",
    "upgrade-interactive": "npm-check --update"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/undataforum/gatsby-theme.git"
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.16.5",
    "@mdx-js/tag": "^0.16.5",
    "@undataforum/components": "^0.23.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby-mdx": "^0.3.6",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-styled-components": "^3.0.5",
    "prop-types": "^15.7.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@mdotasia/eslint-config": "^0.6.0",
    "eslint": "^5.14.1",
    "gatsby": "^2.1.2",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.4",
    "npm-check": "^5.9.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "peerDependencies": {
    "gatsby": "^2.1.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  }
}
