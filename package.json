{
  "name": "@undataforum/gatsby-theme",
  "version": "0.0.0",
  "description": "Gatbsy theme for @UNDataForum websites.",
  "author": "Thilo Maier <maier@un.org>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "rm -rf ./public && gatsby build",
    "format": "pretty-quick",
    "inspect": "node --inspect-brk node_modules/.bin/gatsby develop",
    "lint": "eslint --ext .jsx,.js --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "dependencies": {
    "gatsby": "^2.0.72",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@mdotasia/eslint-config": "^0.5.0",
    "eslint": "^5.10.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0"
  }
}
